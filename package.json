{
  "name": "traktdiscordpresence",
  "version": "3.2.17",
  "description": "Set your Discord presence according to what you are watching on Trakt.tv",
  "license": "GPL-3.0-or-later",
  "author": "Ragnar Lothbrok <ragnarlothbrokjr@proton.me>",
  "homepage": "https://github.com/Valhalla-Development/TraktDiscordPresence#readme",
  "repository": "https://github.com/RagnarLothbrok-Odin/TraktDiscordPresence.git",
  "bugs": {
    "url": "https://github.com/Valhalla-Development/TraktDiscordPresence/issues"
  },
  "type": "module",
  "scripts": {
    "lint": "tsc --noEmit -p tsconfig.json && biome check --files-ignore-unknown=true --diagnostic-level=error --no-errors-on-unmatched src",
    "lint:fix": "biome check --write --no-errors-on-unmatched --files-ignore-unknown=true src",
    "dev": "bun --watch src/index.ts",
    "start": "bun src/index.ts",
    "test": "bun src/index.ts --test",
    "test:movie": "bun src/index.ts --test movie",
    "test:show": "bun src/index.ts --test show",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@types/luxon": "^3.6.2",
    "@xhayper/discord-rpc": "^1.3.0",
    "chalk": "^5.4.1",
    "cli-progress": "^3.12.0",
    "colors": "^1.4.0",
    "lru-cache": "^11.1.0",
    "luxon": "^3.7.1",
    "pretty-ms": "^9.2.0",
    "tmdb-ts": "^2.0.1",
    "trakt.tv": "^8.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.2",
    "@types/cli-progress": "^3.11.6",
    "typescript": "^5.8.3",
    "ultracite": "^5.0.47"
  }
}
